# front-end-development
前端开发功能代码
window.onload=function(){
 	var outer=document.getElementById('subject');//最外层的盒子
	var list=outer.getElementsByTagName('li');

  	for(var i=0;i<list.length;i++){
  	 	list[i].onmouseover=function(){

   		for(var i=0;i<list.length;i++){
   			list[i].className=''; 
   		}

   		this.className='big';
   		}
   	}
}
